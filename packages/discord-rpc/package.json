{
  "name": "dc-rpc",
  "version": "1.6.0",
  "description": "discord-rpc is a powerful library created by Discord, allowing developers to integrate Rich Presence functionality into their Discord applications smoothly.",
  "contributors": [
    "nokarin-dev <nokarin@strivo.xyz>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strivo-dev/discord-rpc.git"
  },
  "keywords": [
    "discord-rpc",
    "discord",
    "javascript"
  ],
  "author": "nokarin-dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strivo-dev/discord-rpc/issues"
  },
  "homepage": "https://github.com/strivo-dev/discord-rpc#readme",
  "main": "./src/index.js",
  "types": "./typings/index.d.ts",
  "scripts": {
    "test": "pnpm run test:typescript && pnpm run test:jest",
    "test:ci": "pnpm run test:typescript",
    "test:typescript": "tsc --noEmit && tsd",
    "test:jest": "jest --coverage",
    "update": "pnpm --recursive update --interactive",
    "update:latest": "pnpm --recursive update --interactive --latest",
    "changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml"
  },
  "jsdelivr": "browser.js",
  "unpkg": "browser.js",
  "exports": {
    ".": {
      "import": {
        "types": "./typings/index.d.mts",
        "default": "./src/index.js"
      },
      "require": {
        "types": "./typings/index.d.ts",
        "default": "./src/index.js"
      }
    }
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "src",
    "typings"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "events": "^3.3.0",
    "node-fetch": "^3.3.2",
    "register-scheme": "github:devsnek/node-register-scheme",
    "tslib": "^2.8.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^6.0.0",
    "@types/node": "^24.10.1",
    "@types/ws": "^8.18.1",
    "chai": "^6.2.1",
    "electron": "^39.2.4",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "mocha": "^11.7.5",
    "prettier": "^3.7.2",
    "tsd": "^0.33.0",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "browser": {
    "net": false,
    "ws": false,
    "uws": false,
    "erlpack": false,
    "electron": false,
    "register-scheme": false,
    "./src/transports/IPC.js": false,
    "./src/transports/WebSocket.js": false,
    "./src/transports/UWS.js": false,
    "./src/transports/Erlpack.js": false,
    "./src/transports/Electron.js": false,
    "./src/transports/register-scheme.js": false,
    "./src/transports/index.js": false,
    "./src/index.js": false,
    "./index.js": false,
    "./package.json": false,
    "./README.md": false,
    "./LICENSE": false
  },
  "publishConfig": {
    "provenance": true
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
